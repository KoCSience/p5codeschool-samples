int x = 10;
x += 10;    // x = x + 10と同じ
println(x); // 20と出力
x -= 5;     // x = x - 5と同じ
println(x); // 15と出力
x *= 2;     // x = x * 2 と同じ
println(x); // 30と出力
x /= 10;    // x = x / 10 と同じ
println(x); // 3と出力
x %= 2;     // x = x % 2 と同じ
println(x); // 1と出力
x++;        // x = x + 1 と同じ
println(x); // 2と出力
x--;        // x = x - 1 と同じ
println(x); // 1と出力
